{
  "name": "my-admin-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "next dev --turbopack",
    "dev:api": "nodemon --watch server --ext js,mjs,cjs,json --exec \"node server/fastify_supabase.js\"",
    "dev": "concurrently -n WEB,API -c \"cyan,magenta\" \"npm:dev:web\" \"npm:dev:api\"",
    "build": "next build",
    "start:web": "next start",
    "start:api": "node server/fastify_supabase.js",
    "start:all": "concurrently -n WEB,API -c \"cyan,magenta\" \"npm:start:web\" \"npm:start:api\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "lucide-react": "^0.539.0",
    "nanoid": "^5.1.5",
    "next": "15.4.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
